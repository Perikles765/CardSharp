/**
 * 
 */
package cardsharp;

import java.util.Comparator;

/**
 * @author BTrainor
 *
 */
public class CardValueComparator implements Comparator<Cards>{

	@Override
	public int compare(Cards o1, Cards o2) {
		// TODO Auto-generated method stub
		return o1.getCardValue() - o2.getCardValue();
	}

}
